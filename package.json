{
  "name": "aventuria-impromptu",
  "version": "0.2.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --run --environment jsdom --passWithNoTests",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "vue": "^3.5.22",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.6",
    "@rushstack/eslint-patch": "^1.13.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.19.21",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
